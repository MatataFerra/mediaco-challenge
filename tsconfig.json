{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "preserve",
		"baseUrl": "./src",
		"paths": {
			"@components/*": ["./components/*"],
			"@atoms/*": ["./components/atoms/*"],
			"@molecules/*": ["./components/molecules/*"],
			"@organisms/*": ["./components/organisms/*"],
			"@layouts/*": ["./layouts/*"],
			"@lib/*": ["./lib/*"],
			"@root/*": ["../*"]
		}
	}
}
